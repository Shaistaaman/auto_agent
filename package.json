{
  "name": "autonomous-devops-incident-responder",
  "version": "1.0.0",
  "description": "AWS AI Agent for autonomous DevOps incident response using Bedrock AgentCore",
  "bin": {
    "devops-agent": "bin/devops-agent.js"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "deploy": "cdk deploy",
    "deploy:dev": "cdk deploy --context environment=dev",
    "deploy:prod": "cdk deploy --context environment=prod",
    "destroy": "cdk destroy",
    "synth": "cdk synth",
    "diff": "cdk diff",
    "bootstrap": "cdk bootstrap",
    "lint": "eslint . --ext .ts",
    "lint:fix": "eslint . --ext .ts --fix"
  },
  "devDependencies": {
    "@types/jest": "^29.5.5",
    "@types/node": "20.6.3",
    "@typescript-eslint/eslint-plugin": "^6.7.0",
    "@typescript-eslint/parser": "^6.7.0",
    "eslint": "^8.49.0",
    "jest": "^29.7.0",
    "ts-jest": "^29.1.1",
    "typescript": "~5.2.2"
  },
  "dependencies": {
    "aws-cdk": "^2.1030.0",
    "aws-cdk-lib": "2.100.0",
    "constructs": "^10.0.0"
  },
  "keywords": [
    "aws",
    "cdk",
    "bedrock",
    "ai-agent",
    "devops",
    "incident-response",
    "hackathon"
  ],
  "author": "Your Name",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/yourusername/autonomous-devops-incident-responder.git"
  }
